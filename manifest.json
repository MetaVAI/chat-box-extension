{
  "manifest_version": 3,
  "name": "Gemini Chatbot",
  "version": "1.0",
  "description": "A chatbot that answers questions about the current page.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html"
}
